/****** Object:  Database [QLSTK]    Script Date: 6/6/2019 10:19:34 AM ******/
CREATE DATABASE [QLSTK]
 CONTAINMENT = NONE
 ON  PRIMARY 
( NAME = N'QLSTK', FILENAME = N'C:\Program Files\Microsoft SQL Server\MSSQL14.MSSQLSERVER\MSSQL\DATA\QLSTK.mdf' , SIZE = 8192KB , MAXSIZE = UNLIMITED, FILEGROWTH = 65536KB )
 LOG ON 
( NAME = N'QLSTK_log', FILENAME = N'C:\Program Files\Microsoft SQL Server\MSSQL14.MSSQLSERVER\MSSQL\DATA\QLSTK_log.ldf' , SIZE = 8192KB , MAXSIZE = 2048GB , FILEGROWTH = 65536KB )
GO

IF (1 = FULLTEXTSERVICEPROPERTY('IsFullTextInstalled'))
begin
EXEC [QLSTK].[dbo].[sp_fulltext_database] @action = 'enable'
end
GO

